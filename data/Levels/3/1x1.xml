<room lights ="false">

  <!--KEYCARD-->
  <object position="-4.5 0 3.0" scale="10" model="Punchcard" material="Pickup" interaction="Pickup"></object>

  <!-- BIN (woooo!) -->
  <object position="2 0.2 0" angles="0 10 90" scale="5" model="Bin" material="CubicleDivider_3rdFloor"></object>

  <!-- STANDARD ONE ROOM CUBICALS-->
  <object position="-2.5 0 4" angles="30 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="-4 0 4" angles="0 30 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>

  <!--Tables-->
  <object position="2.7 0 4.1" angles="0 10 0" scale="20" model="Table" material="FlatGrey"></object>
  <object position="4.5 0 3.5" angles="0 90 0" scale="20" model="Table" material="FlatGrey"></object>
  <!--Seats-->
  <object position="-3.5 0 -3.4" angles="0 85 0" scale="6.5" model ="Seat" material="Seat_3rdFloor"></object>
  <object position="-2.0 0 -2.0" angles="0 0 0" scale="6.5" model ="Seat" material="Seat_3rdFloor"></object>

  <!--Lamps-->
  <object position="4.5 1 2.8" angles="0 0 0" scale="5" model="Lamp" material="Lamp"></object>
  
  <!--Terminals-->
  <object position="2.7 1 4.1" angles="0 10 0" scale="5" model="Terminal" material="Terminal"></object>

  <!-- PAPER -->
  <object position="-0.5 0 -4.5" angles="0 0 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>
  <object position="-2.5 0 -4.2" angles="0 10 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>
  <object position="-1.0 0 -3.8" angles="0 0 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>

  <!-- CABINETS -->
  <object position="-4.5 0 -4.5" angles="0 0 0" scale="25" model="CabinetTop" material="CabinetTop"></object>
  <object position="-4.5 0.1 -3.5" angles="-20 0 0" scale="25" model="CabinetBottom" material="CabinetBottom"></object>
  <object position="-4.5 0.2 -2.5" angles="-40 0 0" scale="25" model="CabinetMiddle" material="CabinetMiddle"></object>
  <object position="-4.5 0 -1.5" angles="0 10 -90" scale="25" model="CabinetShut" material="CabinetShut"></object>
  <object position="-4.5 0 -0.5" angles="0 0 0" scale="25" model="CabinetMiddle" material="CabinetMiddle"></object>

  <!-- Family Picture -->
  <object position="-1 0 1" angles="0 200 90" scale="20" model="PictureFrame" material="Dark_Oak" interaction="Terminal" displaysize="100 50">Wright Family Portrait</object>

</room>
