<room>
  <!-- Wright's Junk Sign-->
  <object position="-5.0 0.3 -1.0" angles="0 -90 0" scale="0.5" model="PersonPlane" material="Wrights_Junk" nonsolid="true"></object>

  <!--Cubical dividers -->
  <object position="-1.5 0 2.5" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="1.5 0 2.5" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="2.25 0 1.75" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="2.25 0 0.25" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="-2.25 0 1.75" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="-2.25 0 0.25" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="-2.25 0 -1.25" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="2.25 0 -1.25" angles="0 90 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="-1.5 0 -2.0" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>
  <object position="1.5 0 -2.0" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider_3rdFloor"></object>

  <!-- COPIER AND PAPER -->
  <object position="-2 0 3.75" angles="0 0 0" scale="17.5" model="Copier" material="Copier"></object>
  <object position="-3 0 4.5" angles="0 0 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>
  <object position="-4.25 0 4.5" angles="0 10 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>

  <!--Tables-->
  <object position="-1.6 0 1.2" angles="0 -90 0" scale="20" model="Table" material="FlatGrey"></object>
  <object position="1.6 0 -0.7" angles="0 90 0" scale="20" model="Table" material="FlatGrey"></object>

  <!--Seats-->
  <object position="-1.2 0 1.2" angles="0 90 0" scale="6.5" model ="Seat" material="Seat_3rdFloor"></object>
  <object position="1.2 0 -0.7" angles="0 -90 0" scale="6.5" model ="Seat" material="Seat_3rdFloor"></object>

  <!--Terminals-->
  <object position="-1.6 1 1.2" angles="0 -90 0" scale="5" model="Terminal" material="Terminal"></object>
  <object position="1.6 1 -0.7" angles="0 95 0" scale="5" model="Terminal" material="Terminal"></object>

  <!--Lamps-->
  <object position="-1.6 1 0.3" angles="0 10 0" scale="5" model="Lamp" material="Lamp"></object>
  <object position="1.6 1 0" angles="0 180 0" scale="5" model="Lamp" material="Lamp"></object>

  <guard>
    <waypoint position="-3.8 0 3"></waypoint>
    <waypoint position="-3.8 0 -3.25"></waypoint>
    <waypoint position="0 0 -3.25"></waypoint>
    <waypoint position="0 0 3.5"></waypoint>
    <waypoint position="3.8 0 3.5"></waypoint>
    <waypoint position="3.8 0 -3.25"></waypoint>
    <waypoint position="-3.8 0 -3.25"></waypoint>
  </guard>

</room>
