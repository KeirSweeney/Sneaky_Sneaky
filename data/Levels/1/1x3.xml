<room>
  <!-- STANDARD ONE ROOM CUBICALS-->
  <object position="4 0 2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="2.5 0 2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="-2.5 0 2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="-4 0 2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="4 0 -2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="2.5 0 -2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="-2.5 0 -2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <object position="-4 0 -2" angles="0 0 0" scale="12.5" model="CubicleDivider" material="CubicleDivider"></object>
  <!--Tables-->
  <object position="-4.5 0 -3.5" angles="0 -90 0" scale="20" model="Table" material="FlatGrey"></object>
  <object position="-4.5 0 3.5" angles="0 -90 0" scale="20" model="Table" material="FlatGrey"></object>
  <object position="4.5 0 -3.5" angles="0 90 0" scale="20" model="Table" material="FlatGrey"></object>
  <object position="4.5 0 3.5" angles="0 90 0" scale="20" model="Table" material="FlatGrey"></object>
  <!--Seats-->
  <object position="-4 0 -3.5" angles="0 90 0" scale="6.5" model ="Seat" material="Seat"></object>
  <object position="-4 0 3.5" angles="0 90 0" scale="6.5" model ="Seat" material="Seat"></object>
  <object position="4 0 -3.5" angles="0 -90 0" scale="6.5" model ="Seat" material="Seat"></object>
  <object position="2.8 0.60 4.2" angles="90 -90 -20" scale="6.5" model ="Seat" material="Seat"></object>
  <!--Lamps-->
  <object position="-4.5 1 -4.2" angles="0 0 0" scale="5" model="Lamp" material="Lamp"></object>
  <object position="-4.6 1 2.5" angles="0 20 0" scale="5" model="Lamp" material="Lamp"></object>
  <object position="4.7 1 4.5" angles="0 225 0" scale="5" model="Lamp" material="Lamp"></object>
  <object position="4.5 1 -2.8" angles="0 180 0" scale="5" model="Lamp" material="Lamp"></object>
  <!--Terminals-->
  <object position="-4.5 1 -3.5" angles="0 -90 0" scale="5" model="Terminal" material="Terminal"></object>
  <object position="-4.5 1 3.8" angles="0 -75 0" scale="5" model="Terminal" material="Terminal"></object>
  <object position="4.5 1 -3.5" angles="0 90 0" scale="5" model="Terminal" material="Terminal"></object>
  <object position="4.5 1 3" angles="0 120 0" scale="5" model="Terminal" material="Terminal"></object>

  <!-- SLACKING OFF CENTRE -->
  <object position="-4.5 0 -0.3" angles="0 90 0" scale="50" model="SofaLength" material="Sofa"></object>
  <object position="-4.5 0 1.6" angles="0 -90 0" scale="6" model="CoolerBase" material="FlatGrey"></object>
  <object position="-4.5 0 1.6" angles="0 -90 0" scale="6" model="CoolerBottle" material="CoolerBottle"></object>

  <!-- RANDOM BOXES-->
  <object position="-0.5 0 4.5" angles="0 12 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>
  <object position="1 0 4.5" angles="0 -3 0" scale="8" model="PrinterBoxes" material="PrinterBoxes"></object>

  <!-- CABINETS -->
  <object position="0.3 0 -4.5" angles="0 -90 0" scale="25" model="CabinetTop" material="CabinetTop"></object>
  <object position="-0.3 0 -4.5" angles="0 -90 0" scale="25" model="CabinetShut" material="CabinetShut"></object>
  <object position="0.9 0 -4.5" angles="0 -90 0" scale="25" model="CabinetMiddle" material="CabinetMiddle"></object>
  <object position="-0.9 0 -4.5" angles="0 -90 0" scale="25" model="CabinetShut" material="CabinetShut"></object>

  <guard>
    <waypoint position="-3 0 3.5"></waypoint>
    <waypoint position="0 0 3.5"></waypoint>
    <waypoint position="0 0 -3.5"></waypoint>
    <waypoint position="-3 0 -3.5"></waypoint>
    <waypoint position="3 0 -3.5"></waypoint>
    <waypoint position="0 0 -3.5"></waypoint>
    <waypoint position="0 0 3.5"></waypoint>
  </guard>


</room>
